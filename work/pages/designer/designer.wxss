/* designer.wxss */
.designer{
  background: #12B2B2;
  padding: 50px 10px;
  position: relative;
  padding-bottom: 25px;
}
.designer .avator{
  display: block;
  width: 90px;height: 90px;
  border-radius: 50%;
  position:absolute;
  top: 10px;
  left: 0;right: 0;
  margin: auto;
}


.designer  .card{  
  background: #fff;
  padding-top:60px ;
  border-radius: 15px;
  font-size: 14px;
  
}
.designer  .card_name{
  padding: 5px 0;
  width: 100%;


  text-align: center;
}
.designer  .card_pro {
  padding: 5px 10px;
  box-sizing: border-box;
  
  font-size: 12px;
  color: #A4A4A4;
  text-align: center;
}
.designer .card_pro .city{
  margin-bottom: 10px;
}
.designer .pro{
  padding: 10px 0;
  display: flex;
  justify-content: space-between;

}
.designer .pro .pros{
  width: 25%;
  color: #000000;
  font-size: 16px;
}
.designer .pro .pros .nub{
  font-size: 16px;
}
.designer .pro .pros .liulan{
  margin-top: 10px;
}
.xuanxiang .title{
  
  border: 1px solid #EDEDED;
  display: flex;
  justify-content: space-between;
}
.xuanxiang .title view{
  height: 40px;
  line-height: 40px;
  width:50%;
  text-align: center;
  font-size: 14px;

}
.xuanxiang .title  .active{
  border-bottom: 1px solid #12B2B2;
  color: #12B2B2;
}
.home .img{
  width: 100%;
  height: 180px;
  background-position: center;
  background-size: cover;
}
.home .floor{
  padding: 5px 10px;
  font-size: 14px;
  padding-top: 10px;
  

}
.home .huxing{
  font-size: 13px;
  color: #797574;
  padding: 5px 10px;
  padding-bottom: 10px;
  
  
}
.footer{
  width: 100%;
  display: flex;
  justify-content: space-between;
}
.footer navigator{
  width: 33%;
}
.footer .footer_list{
  width:100%;
  background: #12B2B2;
  padding: 10px 0;
}
.footer .footer_list .fg{
  display: block;
  margin: 0 auto;
  width: 30px;
  height: 30px;
}
.footer .footer_list .fengge{
  color: #fff;
  font-size: 12px;
  width: 100%;
  text-align: center;
  padding-top: 5px;
}
 .yuyues{
  width: 80%;
  margin: 0 auto;
  background: #12B2B2;
  text-align: center;
  line-height: 24px; 
  color: #fff;
  padding: 10rpx 10px;
  box-sizing: content-box;
}
/* 弹窗 */
.alert_back{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.4);
  z-index: 1000000;
}
.alert_box{
  width: 90%;
  height: 65%; 
  background: #fff;
  position:fixed;
  top: 50%;
  left: 50%;
   margin-top: -50%;
   margin-left: -45%; 
  border-radius: 5px;
  z-index: 1000001;
  
}
.alert_box .price{
  padding:  4%;  
  text-align: left; 
  font-size: 16px;
}
